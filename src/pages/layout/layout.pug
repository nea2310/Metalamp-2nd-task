include ../../components/header/header.pug
include ../../components/footer/footer.pug
include ../../components/head/head.pug


-
  const {header, footer} = require('@pag/layout/layout.json')
doctype html
html(lang = "ru")
  head
    +head()
    block title
      title Layout
  body(class = "layout js-layout")
    div(class = "js-layout__header")
      +header(header)
    block content
      main
    div(class = "layout__footer")
      +footer(footer)




